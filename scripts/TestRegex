#!/bin/bash

RE="^[T]*(\\d\\d):?(\\d\\d):?(\\d\\d(?:[.,]\\d+)?)?(?:Z|([-+]\\d\\d):?(\\d\\d)?)?$"

cat<<EOF > /tmp/test
T12:30:45
T12:30:45.12345
T12:30:45Z
T12:30:45.12345Z
T12:30:45+11:23
T12:30:45.12345+11:23
T12:30:45-20:11
T12:30:45.12345-20:11
T123045
T123045.12345
T123045Z
T123045.12345Z
T123045+1123
T123045.12345+1123
T123045-2011
T123045.12345-2011
12:30:45
12:30:45.12345
12:30:45Z
12:30:45.12345Z
12:30:45+11:23
12:30:45.12345+11:23
12:30:45-20:11
12:30:45.12345-20:11
123045
123045.12345
123045Z
123045.12345Z
123045+1123
123045.12345+1123
123045-2011
123045.12345-2011
EOF

for text in $(cat /tmp/test); do
    callisto/bin/ctest_datetime ${RE} ${text}
done






exit 0
